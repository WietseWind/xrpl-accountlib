{
  "name": "xrpl-accountlib",
  "version": "0.3.7",
  "description": "XRPL Account helper: generation, derivation and signing",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepublish": "npm run clean && npm run test && npm run build",
    "clean": "rm -rf dist",
    "build": "tsc",
    "test": "jest --verbose"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.js.map",
    "dist/**/*.d.ts"
  ],
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "bip32": "*",
    "bip39": "*",
    "bn.js": "*",
    "elliptic": "*",
    "hash.js": "*",
    "ripple-address-codec": "*",
    "ripple-binary-codec": "*",
    "ripple-keypairs": "*",
    "ripple-lib": "*",
    "ripple-secret-codec": "*",
    "ripple-sign-keypairs": "git+https://github.com/WietseWind/node-ripple-sign-keypairs.git",
    "xrpl-secret-numbers": "*"
  },
  "devDependencies": {
    "@types/bip39": "^3.0.0",
    "@types/elliptic": "^6.4.12",
    "@types/jest": "^25.1.3",
    "@types/node": "^14.11.10",
    "jest": "^26.4.2",
    "ts-jest": "^26.4.0",
    "typescript": "^3.8.2"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/WietseWind/xrpl-accountlib.git"
  },
  "bugs": {
    "url": "https://github.com/WietseWind/xrpl-accountlib/issues"
  },
  "homepage": "https://github.com/WietseWind/xrpl-accountlib#readme",
  "license": "MIT",
  "readmeFilename": "README.md",
  "keywords": [
    "xrp",
    "xrpl-ledger",
    "multi sign",
    "sign"
  ]
}
