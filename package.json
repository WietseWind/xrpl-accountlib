{
  "name": "xrpl-accountlib",
  "version": "0.3.5",
  "description": "XRPL Account helper: generation, derivation and signing",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepublish": "npm run clean && npm run test && npm run build",
    "clean": "rm -rf dist",
    "build": "tsc",
    "test": "jest --verbose"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.js.map",
    "dist/**/*.d.ts"
  ],
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "bip39": "latest",
    "bn.js": "latest",
    "elliptic": "latest",
    "hash.js": "latest",
    "ripple-address-codec": "latest",
    "ripple-binary-codec": "latest",
    "ripple-bip32": "latest",
    "ripple-keypairs": "latest",
    "ripple-lib": "latest",
    "ripple-secret-codec": "latest",
    "ripple-sign-keypairs": "git+https://github.com/WietseWind/node-ripple-sign-keypairs.git",
    "xrpl-secret-numbers": "latest"
  },
  "devDependencies": {
    "@types/bip39": "^3.0.0",
    "@types/elliptic": "^6.4.10",
    "@types/jest": "^25.1.3",
    "@types/node": "^12.12.21",
    "jest": "^24.9.0",
    "ts-jest": "^24.2.0",
    "typescript": "^3.7.4"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/WietseWind/xrpl-accountlib.git"
  },
  "bugs": {
    "url": "https://github.com/WietseWind/xrpl-accountlib/issues"
  },
  "homepage": "https://github.com/WietseWind/xrpl-accountlib#readme",
  "license": "MIT",
  "readmeFilename": "README.md",
  "keywords": [
    "xrp",
    "xrpl-ledger",
    "multi sign",
    "sign"
  ]
}
